<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.ttcxy.tang.portal.dao.DtsNoticeDao">
    <resultMap id="NoticeMap" type="net.ttcxy.tang.portal.entity.model.DtsNotice">
        <id column="notice_id" property="noticeId"/>
        <result column="title" property="title"/>
        <result column="url" property="url"/>
        <result column="create_date" property="createDate"/>
        <result column="description" property="description"/>
    </resultMap>
    <select id="selectNoticeAll" resultType="net.ttcxy.tang.portal.entity.model.DtsNotice">
        select notice_id,title,url,create_date,description
        from dts_notice
    </select>
</mapper>