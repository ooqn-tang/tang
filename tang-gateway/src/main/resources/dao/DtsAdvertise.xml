<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.ttcxy.tang.gateway.dao.DtsAdvertiseDao">
    <resultMap id="AdvertiseMap" type="net.ttcxy.tang.gateway.entity.model.DtsAdvertise">
        <id column="advertise_id" property="advertiseId"/>
        <result column="title" property="title"/>
        <result column="url" property="url"/>
        <result column="create_date" property="createDate"/>
        <result column="delivery_time" property="deliveryTime"/>
        <result column="description" property="description"/>
    </resultMap>
    <select id="selectAdvertiseAll" resultType="net.ttcxy.tang.gateway.entity.model.DtsAdvertise">
        select advertise_id,title,url,create_date,delivery_time,description
        from dts_advertise
    </select>
</mapper>